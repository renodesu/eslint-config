{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 20",
  "_version": "20.1.0",
  "compilerOptions": {
    "lib": [
      "es2023"
    ],
    "module": "node16",
    "target": "es2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node16",
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src/**/*",
    "eslint.config.js",
    "prettier.config.js",
  ],
}
